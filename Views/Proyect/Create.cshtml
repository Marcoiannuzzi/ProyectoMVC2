@model ProyectosModelview

<section class="page-section vh-80 mt-5 bg-dark text-white p-5">
<h1>Create a new Proyect</h1>


<form class="row g-3 needs-validation" novalidate asp-action="Create">

    <partial name="_ProyectFormPartialView" model="@Model"/>

    <div class="col-12">
        <button class="btn btn-primary" type="submit">Create Product</button>
   </div>

</form>


<a class="btn btn-outline-primary m-2" asp-action="Index" asp-controller="Proyect">Go Back</a>

</section>
