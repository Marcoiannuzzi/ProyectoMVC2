@model EducationViewModel


<div class="col-md-4">
    <label for="Name" class="form-label">Name</label>
    <input asp-for="Name" type="text" class="form-control" id="Name" required>
    @Html.ValidationMessageFor(x=>x.Name,"",new{@class = "text-danger"})
</div>
<div class="col-md-4">
    <label for="Institution" class="form-label">Institution</label>
    <input asp-for="Institution" type="text" class="form-control" id="Institution" required>
    @Html.ValidationMessageFor(x=>x.Institution,"",new{@class = "text-danger"})
</div>
<div class="col-md-4">
    <label for="Description" class="form-label">Description</label>
    <input asp-for="Description" type="text" class="form-control" id="Description" required>
    @Html.ValidationMessageFor(x=>x.Description,"",new{@class = "text-danger"})
</div>
<div class="col-md-4">
    <label for="Start" class="form-label">Start</label>
    <input asp-for="Start" type="date" class="form-control" id="Start" required>
    @Html.ValidationMessageFor(x=>x.Start,"",new{@class = "text-danger"})
</div>
<div class="col-md-4">
    <label for="End" class="form-label">End</label>
    <input asp-for="End" type="date" class="form-control" id="End" required>
    @Html.ValidationMessageFor(x=>x.End,"",new{@class = "text-danger"})
</div>
